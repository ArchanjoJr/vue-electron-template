{
  "author": "Muhammad Ubaid Raza <mubaidr@gmail.com>",
  "bugs": {
    "url": "https://github.com/mubaidr/vue-electron-template/issues"
  },
  "build": {
    "appId": "org.mubaidr.vue-electron-template",
    "directories": {
      "output": "build"
    },
    "dmg": {
      "contents": [
        {
          "path": "/Applications",
          "type": "link",
          "x": 410,
          "y": 150
        },
        {
          "type": "file",
          "x": 130,
          "y": 150
        }
      ]
    },
    "files": [
      "dist/**/*"
    ],
    "linux": {
      "icon": "build/icons",
      "publish": [
        "github"
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns",
      "publish": [
        "github"
      ]
    },
    "productName": "productName",
    "win": {
      "icon": "build/icons/icon.ico",
      "publish": [
        "github"
      ]
    }
  },
  "dependencies": {
    "bulma-addons": "^0.1.12",
    "bulma-pro": "^0.1.4",
    "vue": "^2.5.17",
    "vue-electron": "^1.0.6",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "description": "An electron-vue project",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "colors": "^1.3.2",
    "cross-env": "^5.2.0",
    "devtron": "^1.4.0",
    "electron": "~3.0.6",
    "electron-builder": "^20.29.0",
    "electron-debug": "^2.0.0",
    "electron-rebuild": "^1.8.2",
    "eslint": "^5.8.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-vue": "^4.7.1",
    "node-sass": "^4.9.4",
    "ora": "^3.0.0",
    "postcss": "^7.0.5",
    "rollup": "^0.66.6",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-babel-minify": "^6.1.1",
    "rollup-plugin-clean": "^1.0.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-copy": "^0.2.3",
    "rollup-plugin-css-only": "^0.4.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-sass": "^0.9.2",
    "rollup-plugin-typescript": "^1.0.0",
    "rollup-plugin-vue": "^4.3.2",
    "serve-handler": "^5.0.5",
    "tslib": "*",
    "typescript": "^3.1.4",
    "vue-devtools": "^5.0.0-beta.1",
    "vue-template-compiler": "^2.5.17"
  },
  "license": "MIT",
  "main": "./dist/main/index.js",
  "name": "vue-electron-template",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mubaidr/vue-electron-template.git"
  },
  "scripts": {
    "build": "npm run pack && electron-builder",
    "build:dir": "npm run pack && electron-builder --dir",
    "dev": "node vue-electron/dev-runner.js",
    "lint": "eslint --ext .js,.vue --fix src test",
    "pack": "rollup -c vue-electron/rollup.config.js",
    "app-deps": "electron-builder install-app-deps",
    "rebuild-electron": "./node_modules/.bin/electron-rebuild"
  },
  "version": "0.0.1"
}
